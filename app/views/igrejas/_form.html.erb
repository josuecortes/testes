<%= simple_form_for @igreja, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :cnpj %>
  <%= f.input :nome %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                igrejas_path, :class => 'btn' %>
  </div>
<% end %>
